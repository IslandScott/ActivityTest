<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"

    >

    <Button
        android:id = "@+id/button_1"
        android:layout_width = "match_parent"
        android:layout_height = "wrap_content"
        android:text = "显示Toast信息"
        />
    <Button
        android:id = "@+id/button_start"
        android:layout_width = "match_parent"
        android:layout_height = "wrap_content"
        android:text = "开始另外一个活动"
        />
    <Button
        android:id = "@+id/button_hidden"
        android:layout_width = "match_parent"
        android:layout_height = "wrap_content"
        android:text = "开始隐藏式活动"
        />
    <Button
        android:id = "@+id/button_internet"
        android:layout_width = "match_parent"
        android:layout_height = "wrap_content"
        android:text = "开启网页"
        />
    <Button
        android:id = "@+id/button_4"
        android:layout_width = "match_parent"
        android:layout_height = "wrap_content"
        android:text = "用另外一种方式开启网页"
        />

    <Button
        android:id = "@+id/button_5"
        android:layout_width = "match_parent"
        android:layout_height = "wrap_content"
        android:text = "返回数据给上一个结构"
        />

    <Button
        android:id = "@+id/button_6"
        android:layout_width = "match_parent"
        android:layout_height = "wrap_content"
        android:text = "杀死所有进程"
        />

    <Button
        android:id = "@+id/button_7"
        android:layout_width = "match_parent"
        android:layout_height = "wrap_content"
        android:text = "UIWidgetTest"
        />

    <Button
        android:id = "@+id/list_view"
        android:layout_width = "match_parent"
        android:layout_height = "wrap_content"
        android:text = "ListView"
        android:textAllCaps="false"
        />

    <Button
        android:id = "@+id/button_20"
        android:layout_width = "match_parent"
        android:layout_height = "wrap_content"
        android:text = "RecyclerView"
        android:textAllCaps="false"
        />
    <Button
        android:id = "@+id/button_21"
        android:layout_width = "match_parent"
        android:layout_height = "wrap_content"
        android:text = "Dialog"
        android:textAllCaps="false"
        />
    <Button
        android:id = "@+id/button_fragment"
        android:layout_width = "match_parent"
        android:layout_height = "wrap_content"
        android:text = "FragmentTest"
        android:textAllCaps="false"
        />
    <Button
        android:id = "@+id/news_test"
        android:layout_width = "match_parent"
        android:layout_height = "wrap_content"
        android:text = "NewsTest"
        android:textAllCaps="false"
        />


</LinearLayout>